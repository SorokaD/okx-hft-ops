Rename this file to .env.example if your editor blocks dotfile creation.

# Global
COMPOSE_PROJECT_NAME=okx_hft_ops

# Network
HFT_NETWORK=hft_network

# MinIO
MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=minio123
MINIO_CONSOLE_PORT=9001
MINIO_API_PORT=9000

# MLflow
MLFLOW_PORT=5000
MLFLOW_BACKEND_STORE_URI=sqlite:////mlflow/mlruns.db
MLFLOW_S3_ENDPOINT_URL=http://minio:9000
AWS_ACCESS_KEY_ID=minio
